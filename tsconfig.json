{
	"compilerOptions": {
		"types": ["svelte", "node"],
		"jsx": "react-jsx",
		"baseUrl": ".",
		"module": "ESNext",
		"target": "ESNext",
		"allowJs": true,
		"noImplicitAny": true,
		"moduleResolution": "node",
		"importHelpers": true,
		"isolatedModules": true,
		"strict": true,
		"lib": ["DOM", "ESNext"]
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "public"]
}
